{
    "name": "neta",
    "description": "A simple and neat theme for the Ghost inspired by the Aegea blog engine and Teletype.in",
    "demo": "https://amashkov.ru/neta",
    "version": "0.2.7",
    "engines": {
        "ghost": ">=3.0.0",
        "ghost-api": "v3"
    },
    "license": "MIT",
    "author": {
        "name": "Alexander Mashkov",
        "email": "somexprt@gmail.com",
        "url": "https://amashkov.ru/"
    },
    "gpm": {
        "type": "theme",
        "categories": [
            "Minimal",
            "Blog"
        ]
    },
    "keywords": [
        "ghost",
        "theme",
        "ghost-theme"
    ],
    "config": {
        "posts_per_page": 10,
        "image_sizes": {
            "xxs": {
                "width": 30
            },
            "xs": {
                "width": 100
            },
            "s": {
                "width": 300
            },
            "m": {
                "width": 600
            },
            "l": {
                "width": 1000
            },
            "xl": {
                "width": 2000
            }
        }
    }
}