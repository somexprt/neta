{{!< default}}

{{#page}}

	<section class="page-content gh-canvas">
        <h1>{{title}}</h1>
		{{content}}
	</section>

    {{#contentFor "header-cover"}}
    <div class="gh-head-cover gh-head-bg-image" style="background-image: url({{img_url feature_image}})"></div>
    {{/contentFor}}

{{/page}}

<main id="site-main" class="site-main outer">
    <div class="inner posts">
        {{> loop}}
        {{pagination}}
    </div>
</main>